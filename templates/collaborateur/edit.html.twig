{% extends 'base.html.twig' %}
{% block title %}Modifier le collaborateur{% endblock %}

{% block body %}
    <h1>Modifier le collaborateur</h1>

    {{ form_start(registrationForm) }}
        {{ form_row(registrationForm.email) }}
        {{ form_row(registrationForm.nom) }}
        {{ form_row(registrationForm.prenom) }}
        {{ form_row(registrationForm.tel) }}
        {{ form_row(registrationForm.service) }}
        {{ form_row(registrationForm.roles) }}
        <button class="btn">Modifier le collaborateur</button>
    {{ form_end(registrationForm) }}

    <a class="btn btn-dark" href="{{ path('artistes_index') }}">Retour</a>
{% endblock %}
