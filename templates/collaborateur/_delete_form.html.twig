<form method="post" action="{{ path('collaborateur_delete', {'id': collaborateur.id}) }}" onsubmit="return confirm('Voulez-Vous vraiment supprimer ce Collaborateur ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ collaborateur.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>